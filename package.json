{
  "name": "@eliasnorrby/git-config",
  "version": "1.2.4",
  "description": "",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8"
  },
  "files": [
    "index.js",
    "setup.js",
    "gitignore",
    "gitattributes"
  ],
  "main": "index.js",
  "bin": "setup.js",
  "scripts": {
    "lint": "tsc",
    "semantic-release": "semantic-release",
    "check-format": "prettier --list-different '**/*.{js,jsx,ts,tsx,html,vue,css,less,scss,graphql,yaml,yml,json,md,mdx}'",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,html,vue,css,less,scss,graphql,yaml,yml,json,md,mdx}'"
  },
  "keywords": [
    "config",
    "git"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eliasnorrby/git-config.git"
  },
  "author": "Elias Norrby",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eliasnorrby/git-config/issues"
  },
  "homepage": "https://github.com/eliasnorrby/git-config#readme",
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@eliasnorrby/commitlint-config": "^2.2.3",
    "@eliasnorrby/prettier-config": "^3.0.0",
    "@eliasnorrby/semantic-release-config": "^2.2.4",
    "@types/yargs": "^15.0.0",
    "husky": "^4.0.10",
    "prettier": "^1.19.1",
    "semantic-release": "^16.0.2",
    "typescript": "^3.7.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@eliasnorrby/log-util": "^1.1.0",
    "yargs": "^15.0.2"
  }
}
